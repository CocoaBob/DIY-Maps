source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

workspace 'DIY Maps'
project 'DIY Maps for iOS/DIY Maps for iOS.xcodeproj'

target 'DIY Maps for iOS' do
	pod 'Base64nl'
	pod 'CBDoubleTapAndPanGestureRecognizer', '~> 1.0'
	pod 'CBZipFile', '~> 1.1'
	pod 'Fingertips', '~> 0.4'
	pod 'KissXML'
	pod 'MHWDirectoryWatcher', '~> 0.0.6'
	pod 'SBRXCallbackURLKit'
	pod 'ZKRevealingTableViewCell', :git => 'git://github.com/Nailer/ZKRevealingTableViewCell'
	pod 'ZipKit'
end
